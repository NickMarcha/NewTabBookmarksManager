{
  "name": "New Tab Bookmarks",
  "description": "View and manage your bookmarks when you open a new tab",
  "version": "1.0",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-32x32.png",
    "128": "favicon-96x96.png"
  }
}